{
  "openapi": "3.0.0",
  "info": {
    "title": "Dochat AI Server API",
    "description": "Dochat API for creating and managing document chats.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8060/v1"
    }
  ],
  "paths": {
    "/chats": {
      "get": {
        "summary": "List all chats",
        "operationId": "listChats",
        "tags": [
          "chats"
        ],
        "responses": {
          "200": {
            "description": "An array of chats",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Chat"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Chat": {
        "type": "object",
        "required": [
          "id",
          "name"
        ],
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string"
          },
          "tag": {
            "type": "string"
          }
        }
      }
    }
  }
}
